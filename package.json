    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "eslint": "^1.7.2",
    "eslint-config-angular": "^0.5.0",
    "eslint-config-moment": "^1.5.0",
    "eslint-loader": "1.1.0",
    "eslint-plugin-angular": "^1.3.0",
    "ng-annotate-loader": "^0.1.0",
    "onchange": "^2.0.0",
    "osx-notifier": "^0.2.2",
    "webpack": "1.12.9"
